def are_lists_equal(list1,list2):
    """this function gets 2 inputs (int/float only) and returns "True" if the 2 lists have the same objects
     (the order doesn't matter). otherwise the function returns False"""
    list1.sort()
    list2.sort()
    if list1==list2:
        return True
    else:
        return False
list1 = [0.6, 1, 2, 3]
list2 = [3, 2, 0.6, 1]
list3 = [9, 0, 5, 10.5]

print(are_lists_equal(list1, list2))
print(are_lists_equal(list1, list3))

